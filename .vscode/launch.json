{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "C#: Debug",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build",
        "program": "${workspaceFolder}/bin/Debug/net8.0/testeiel.dll",
        "cwd": "${workspaceFolder}",
        "requireExactSource": false,
        "stopAtEntry": false,
        "env": {
          "ASPNETCORE_ENVIRONMENT": "Development"
        },
        "sourceFileMap": {
          "/Views": "${workspaceFolder}/Views"
        }
      }
    ]
  }
  